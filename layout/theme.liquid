<!DOCTYPE html>
<!--

{% echo shop.name %}
Design by Matto studio

{{ 'now' | date: "%Y" }}

-->
<html lang="{{ shop.locale }}">  
<head>  
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
  {%- if template.name != "index" and template.name != "cart" -%}
  	<title>{{ shop.name }} — {{ page_title }}</title>
  {%- elsif template.name == "cart" -%}
  	<title>{{ shop.name }} — {% if cart.item_count == 0 %}Empty {% endif %}Cart</title>
  {%- else -%}
  	<title>{{ shop.name }}</title>
  {%- endif -%}
  <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no, shrink-to-fit=no">
  <link rel="canonical" href="{{ canonical_url }}" />
  {%- if settings.favicon.size > 0 -%}
<link rel="shortcut icon" href="{{ 'favicon.png' | asset_url }}">  {%- endif -%}
  {%- if page_description -%}
    <meta name="description" content="{{ page_description | escape }}" />
  {%- else -%}
  	<meta name="description" content="{{ shop.description }}" />
  {%- endif -%}  
  {%- if template contains '404' or template contains 'cart' or template contains 'search' -%}
	<meta name="robots" content="noindex">
  {%- endif -%}
  {{ 'style.css'  | asset_url | stylesheet_tag }}
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
  {{ 'current-device-browser.min.js' | asset_url | script_tag }}
  {%- if template == 'product' -%}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}
  {%- endif -%}
  {{ 'global.js' | asset_url | script_tag }}
  {%- if template == "cart" and cart.item_count > 0 -%}
  {{ 'global-cart.js' | asset_url | script_tag }}
  {%- endif -%} 
  {%- if template == 'product' -%}
  {{ '//cdnjs.cloudflare.com/ajax/libs/jquery.lazyload/1.9.1/jquery.lazyload.min.js' | script_tag }}
  <link rel="preload" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" as="style" onload="this.onload=null;this.rel='stylesheet'">
  <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.5.0/js/swiper.min.js"></script>
 

  {%- endif -%}  
  {{ content_for_header }}
</head>

<body class="template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}
             {%- if template.suffix.size > 0 -%}
             template-{{ template.suffix }}
             {% endif %}
             {%- if template == 'cart' and cart.item_count == 0 -%}
             template-cart-empty
             {%- endif -%}">
  
  <div id="loader"
       style="position: fixed;

              top: 0;
              left: 0;
              width: 100vw;
              height: 100%;
              background-color: white;
              z-index: 99999999;">
	<script type="text/javascript">
      setTimeout(function(){
		$("#loader").delay(10).fadeOut(function(){
          $(this).remove();
		});
      }, 500);
    </script>
  </div>
  <!--Header-->
  <header>
	<div class="menu">
      <a href="home"class="logo">
        <svg id="fbba7f3a-02ee-4ff9-a8e9-30c231503c85" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 52 52"><defs>
          <style>.eaaba417-688f-4f4c-852e-efb72039433f{fill:none;}.f41ef72a-d354-4d42-b1f9-8b0262852005{clip-path:url(#f6b19605-66f0-4e87-a882-5b1427a30640);}.f116fa49-930c-4cdd-b8b6-968e385ed3e9{fill:#231f20;}</style>
          </defs><g class="f41ef72a-d354-4d42-b1f9-8b0262852005"><path class="f116fa49-930c-4cdd-b8b6-968e385ed3e9" d="M19.29,26.63h1.6l-.81-2.32ZM45,25.82a2,2,0,1,0-4.07,0A2,2,0,0,0,43,28a2.22,2.22,0,0,0,1-.19,2.16,2.16,0,0,0,1-2m2.53,0a3.51,3.51,0,0,1-.5,1.86,3.11,3.11,0,0,1-.81.92,5,5,0,0,1-3.29.92c-2.39,0-3.37-.85-3.87-1.54a3.24,3.24,0,0,1-.65-2.07A3.71,3.71,0,0,1,39,23.75a4.5,4.5,0,0,1,3.81-1.6,6.69,6.69,0,0,1,2.55.4,3.41,3.41,0,0,1,2.13,3.26m-9.09-1.87H36v5.32H33.53V23.94H31.17V22.39h7.26Zm-7.87,0H28.22v5.32H25.74V23.94H23.3V22.39h7.26ZM21.47,22.4l3,6.86H21.77l-.37-1.11H18.78l-.37,1.11H15.72l3-6.86Zm-6.35,6.86H12.7V24.87l-1.63,4.39H9.18l-1.6-4.37v4.37H5.17V22.4H8.42l1.73,4.48,1.73-4.48h3.24ZM52,0H0V52H52Z"/></g></svg>
      </a>
      <div class="center">	
      	<div><a href="../collections/publishing" {% if template == 'collection' %}class="active" {% endif %} >publishing</a></div>
       	<div><a href="../pages/studio" {% if template == 'page.studio' %}class="active" {% endif %} >studio</a></div> 
        {% unless template == 'index' or page.url == '/pages/contact' %}          <div class="info">
            {% if template == 'page.studio' %}MATTO is a creative agency, based in Paris <br>photography — art direction — publishing — video{% else %}
            contemporary art magazine & publishing as artistic practice{% endif %}
           </div> 
       	{% endunless %}
        {% unless page.url == '/pages/contact' %}
        <div class="contact">
          <a href="../pages/contact">contact</a><br>
	{% endunless %}
  
        
	    {% if template == 'product'%}
        <div id="cart"{% if template == 'cart' %}class="active" {% endif %} class="c"> <a href="/cart">cart<span class="cart-total">
         
        {%- if cart.item_count == 0 -%}
      	0
        {%- else -%}
    	
        {{- cart.item_count -}}
      
        {%- endif -%}
           
      	</span></a></div> 
        
        
        
        {% endif %}

              {% if template == 'index'%}
        <div style="opacity:0; pointer-events:none;"id="cart"{% if template == 'cart' %}class="active" {% endif %} class="c"> <a href="/cart">cart<span class="cart-total">
         
        {%- if cart.item_count == 0 -%}
      	0
        {%- else -%}
    	
        {{- cart.item_count -}}
      
        {%- endif -%}
           
      	</span></a></div> 
        
        
        
        {% endif %}

          
        
          {% if template == 'collection'%} 
        
        <div id="cart"{% if template == 'cart' %}class="active" {% endif %} class="c"> <a href="/cart">cart<span class="cart-total">
         
        {%- if cart.item_count == 0 -%}
      	0
        {%- else -%}
    	
        {{- cart.item_count -}}
      
        {%- endif -%}
           
      	</span></a></div>
            {% endif %}
        
          {% if template == 'cart'%}
        
        <div id="cart"{% if template == 'cart' %}class="active" {% endif %} class="c"> <a href="/cart">cart<span class="cart-total">
         
        {%- if cart.item_count == 0 -%}
      	0
        {%- else -%}
    	
        {{- cart.item_count -}}
      
        {%- endif -%}
           
      	</span></a></div>
        
        
        
        {% endif %}
      	</div>
      </div>
            <div class="none">
                {% unless template == 'index' or page.url == '/pages/contact' %}          <div class="info">
            {% if template == 'page.studio' %}MATTO is a creative agency, based in Paris <br>photography — art direction — publishing — video{% else %}
            contemporary art magazine & publishing as artistic practice {% endif %}
           </div> 
            </div>
       	{% endunless %}
	</div>
  </header>
  {{ content_for_layout }}
  <footer>
  </footer>
  {%- if template contains 'product' -%}
  {{ 'global-product.js' | asset_url | script_tag }}
{%- endif -%} 
{%- if template == 'product' -%}
	{% render 'ajaxify-cart' %}  
{%- endif -%}{% render 'mws_apo_init' %}
  
</body>
  <script>
function updatePrice(){
var prices = document.querySelectorAll(".price-item");


if (!prices)return;

for (var each of prices){
each.innerText = each.innerText.replace(',00', ',00');
}
}

 window.onload = function() {
setTimeout(updatePrice, 50);
            }

</script>
</html>